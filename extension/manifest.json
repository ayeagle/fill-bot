{
  "name": "React Chrome Extension",
  "version": "1.0.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "Background.js"
  },
  "permissions": [
    "storage"
    ],
  "web_accessible_resources": [
    {
      "resources": ["images/fillbot.png"],
      "matches": [ "https://*/*" ]

    }
  ]

}